{
  "name": "laravel-ready/laravel-vitepress",
  "description": "Serve VitePress documentation in Laravel with middleware and authentication support",
  "keywords": [
    "laravel",
    "vitepress",
    "documentation",
    "docs"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Laravel Ready",
      "email": "info@laravelready.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/routing": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "larastan/larastan": "^2.0|^3.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.0|^3.0|^4.1",
    "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0",
    "phpstan/extension-installer": "^1.3",
    "phpstan/phpstan": "^1.10|^2.0",
    "phpstan/phpstan-deprecation-rules": "^1.1|^2.0",
    "phpstan/phpstan-phpunit": "^1.3|^2.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelReady\\VitePress\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelReady\\VitePress\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelReady\\VitePress\\VitePressServiceProvider"
      ],
      "aliases": {
        "VitePress": "LaravelReady\\VitePress\\Facades\\VitePress"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
    "phpstan": "@analyse"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
